DISABLE_ON="Darwin"
APP_STANDALONE="true"
APP_FULL_NAME="Zabbix monitoring system"
APP_NAME="zabbix"
APP_SHA="9090472e5f78fd828bf89d5558ceedd60db74443"
APP_VERSION="2.4.7"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}.tar.gz"
APP_CONFIGURE_ARGS="--enable-proxy --enable-server --enable-agent --disable-largefile --with-jabber --with-libcurl --with-sqlite3 --with-iconv=${PREFIX} --with-iconv-include=${PREFIX}/include --with-iconv-lib=${PREFIX}/lib"
APP_REQUIREMENTS="pkgconf gmake libiconv curl iksemel readline-dynamic sqlite"
APP_EXPORTS="zabbix_get zabbix_sender zabbix_agent zabbix_agentd zabbix_proxy zabbix_server"
