inherit mariadb

DEF_DISABLE_ON="Linux"
DEF_FULL_NAME="MariaDB 10.1 client"
DEF_SUFFIX="-client"
DEF_CONFIGURE_ARGS="-DBUILD_CONFIG=mysql_release -DINSTALL_INCLUDEDIR=include/mysql -DINSTALL_INFODIR=share/info -DINSTALL_MYSQLSHAREDIR=share/mysql -DMYSQL_MAINTAINER_MODE=OFF -DINSTALL_SCRIPTDIR=bin -DDEFAULT_CHARSET=utf8 -DDEFAULT_COLLATION=utf8_general_ci -DENABLED_LOCAL_INFILE=ON -DENABLE_DEBUG_SYNC=OFF -DWITH_SAFEMALLOC=OFF -DWITH_UNIT_TESTS=OFF -DWITH_VALGRIND=OFF -DWITH_LIBWRAP=OFF -DWITH_INNODB_LZ4=YES -DGRN_WITH_LZ4=YES -DWITH_INNODB_BZIP2=YES -DWITH_INNODB_LZMA=YES -DWITH_INNODB_LZO=YES -DINSTALL_SYSCONFDIR=${SERVICE_DIR}/etc -DTMPDIR=/tmp -DMYSQL_DATADIR=${SERVICE_DIR}/data -DWITH_SSL=yes -DWITH_INNODB_DISALLOW_WRITES=ON -DENABLED_PROFILING=OFF -DCOMPILATION_COMMENT=VerKnowSys -DHAVE_STDCXX11=YES -DWITH_EMBEDDED_SERVER=OFF -DCMAKE_SKIP_BUILD_RPATH=YES -DWITHOUT_EXAMPLE_STORAGE_ENGINE=1 -DWITH_JEMALLOC=YES -DWITH_PCRE=system -DWITH_ZLIB=system -DWITH_INNODB_SNAPPY=YES -DSECURITY_HARDENED=ON -DWITHOUT_SERVER=ON -DPLUGIN_AUTH_PAM=OFF"
DEF_USELESS="${DEF_USELESS} support-files"
