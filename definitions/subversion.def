DEF_DISABLE_ON="Darwin"
DEF_FULL_NAME="Subversion"
DEF_NAME="subversion"
DEF_VERSION="1.12.0"
DEF_SHA="2dfecac56f18951182dbee4a4f14d27f27ebcdb7"
DEF_SOURCE_PATH="${MAIN_SOURCE_REPOSITORY}${DEF_NAME}-${DEF_VERSION}.tar.bz2"
DEF_ORIGIN="http://ftp.man.poznan.pl/apache/${DEF_NAME}/${DEF_NAME}-${DEF_VERSION}.tar.bz2"
DEF_CONFIGURE_ARGS="--enable-optimize --disable-nls --with-sqlite=${PREFIX} --with-utf8proc=${PREFIX} --with-apr=${PREFIX} --with-apr-util=${PREFIX} --with-zlib=${PREFIX} --with-lz4=${PREFIX} --with-expat=${PREFIX}/include:${PREFIX}/lib:expat --with-utf8proc=internal --with-libmagic=/usr --with-boost=no"
DEF_REQUIREMENTS="make zlib lz4 expat apr db apr-util sqlite"
DEF_USELESS="lib/*.a lib/apr-util-1/*.a include lib/pkgconfig share/pkgconfig man build-1"
DEF_EXPORTS="svn svnadmin svndumpfilter svnlook svnrdump svnserve svnsync svnversion"
