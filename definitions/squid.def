DEF_STANDALONE=YES
DEF_FULL_NAME="Squid proxy"
DEF_NAME="squid"
DEF_VERSION="5.9"
DEF_SHA="38a723c3ef8776ef3c880772d85020af8b345a84"
DEF_SOURCE_PATH="${MAIN_SOURCE_REPOSITORY}${DEF_NAME}-${DEF_VERSION}.tar.xz"
DEF_ORIGIN="http://www.squid-cache.org/Versions/v4/${DEF_NAME}-${DEF_VERSION}.tar.xz"
DEF_REQUIREMENTS="gmp expat nettle openssl perl"
DEF_CONFIGURE_ARGS="--with-openssl=${PREFIX} --localstatedir=${SERVICE_DIR}/var --datadir=${SERVICE_DIR}/data --sysconfdir=${SERVICE_DIR}/etc --with-logdir=${SERVICE_DIR}/log --with-pidfile=${SERVICE_DIR}/service.pid --with-swapdir=${SERVICE_DIR}/swap --bindir=${PREFIX}/sbin --sbindir=${PREFIX}/sbin --libexecdir=${PREFIX}/libexec --with-default-user=www --with-aio --with-large-files --with-dl --enable-x-accelerator-vary --enable-cpu-profiling --enable-pf-transparent --enable-build-info --enable-loadable-modules --enable-removal-policies=\"lru heap\" --disable-select --disable-poll --disable-epoll --disable-linux-netfilter --disable-linux-tproxy --disable-http-violations --disable-translation --disable-arch-native --disable-auth --without-heimdal-krb5 --without-mit-krb5 --without-gss --without-gnutls --enable-static=no --without-libxml2 --without-systemd"
DEF_USEFUL="bin/perl* bin/openssl"
DEF_USELESS="include lib/*.{a,la} share/aclocal man docs"
DEF_EXPORTS="squid squidclient purge cachemgr.cgi cert_tool cert_valid.pl diskd ext_delayer_acl ext_file_userip_acl ext_kerberos_ldap_group_acl ext_sql_session_acl ext_time_quota_acl ext_unix_group_acl ext_wbinfo_group_acl helper-mux log_db_daemon log_file_daemon storeid_file_rewrite unlinkd url_fake_rewrite url_fake_rewrite.sh url_lfs_rewrite"
DEF_LINKER_FLAGS="-lpthread"
