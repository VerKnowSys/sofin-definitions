DEF_FULL_NAME="MySQL 5"
DEF_ORIGIN="http://dev.mysql.com/downloads/mysql/"
DEF_STANDALONE=YES
DEF_SHA="b352b44385668f0d327d3f275f33f660d85497b3"
DEF_NAME="mysql"
DEF_VERSION="5.6.34"
DEF_SOURCE_PATH="${MAIN_SOURCE_REPOSITORY}${DEF_NAME}-${DEF_VERSION}.tar.gz"
DEF_REQUIREMENTS="ccache zlib bzip2 xz bzip2 perl expat cmake"
DEF_CONFIGURE_METHOD="cmake"
DEF_CONFIGURE_ARGS="-DDEFAULT_CHARSET=utf8 -DDEFAULT_COLLATION=utf8_general_ci -DENABLED_LOCAL_INFILE=off -DENABLE_DEBUG_SYNC=off -DWITH_UNIT_TESTS=off"
DEF_USEFUL="bin/mysql*"
DEF_EXPORTS="mysql mysql_config mysql_config_editor mysql_convert_table_format mysql_find_rows mysql_fix_extensions mysql_plugin mysql_secure_installation mysql_setpermission mysql_waitpid mysql_zap mysqlaccess.conf mysqlbug mysqld mysqld_multi mysqld_safe mysqladmin mysqlshow mysqlimport mysqlbinlog mysqldump mysqlaccess mysqlcheck mysqlhotcopy mysql_upgrade mysql_install_db mysqldumpslow mysqlslap mysqltest msql2mysql my_print_defaults"
DEF_CONFLICTS_WITH="Mysql Percona-server Mariadb"
if [ "Darwin" = "${SYSTEM_NAME}" ]; then
    DEF_SYSTEM_SPECIFIC_CXXFLAGS="-arch=x86_64"
fi
