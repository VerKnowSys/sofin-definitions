DEF_FULL_NAME="OpenSSH"
DEF_NAME="openssh"
DEF_VERSION="9.9p1"
DEF_SHA="5ded7eb0add0b02b5d1a1c4bf5cb2c89d2117b53"
DEF_SOURCE_PATH="${MAIN_SOURCE_REPOSITORY}${DEF_NAME}-${DEF_VERSION}.tar.gz"
DEF_ORIGIN="https://cdn.openbsd.org/pub/OpenBSD/OpenSSH/portable/${DEF_NAME}-${DEF_VERSION}.tar.gz"
DEF_CONFIGURE_ARGS="--with-ssl-engine --with-pid-dir=${SERVICE_DIR} --with-zlib=${PREFIX} --with-ssl-dir=${PREFIX} --with-security-key-builtin --with-pie --disable-etc-default-login"
DEF_REQUIREMENTS="openssl libcbor zlib libfido2"
DEF_EXPORTS="c_rehash openssl scp sftp ssh ssh-add ssh-agent ssh-keygen ssh-keyscan sshd"
