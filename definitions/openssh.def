DEF_FULL_NAME="OpenSSH"
DEF_NAME="openssh"
DEF_VERSION="9.9p1"
DEF_SHA="5ded7eb0add0b02b5d1a1c4bf5cb2c89d2117b53"
DEF_SOURCE_PATH="${MAIN_SOURCE_REPOSITORY}${DEF_NAME}-${DEF_VERSION}.tar.gz"
DEF_ORIGIN="https://cdn.openbsd.org/pub/OpenBSD/OpenSSH/portable/${DEF_NAME}-${DEF_VERSION}.tar.gz"
DEF_CONFIGURE_ARGS="--with-ssl-engine --with-pid-dir=${SERVICE_DIR} --with-zlib=${PREFIX} --with-ssl-dir=${PREFIX} --with-security-key-builtin --disable-etc-default-login --with-libedit=${PREFIX} --without-rpath --without-shadow" #  --with-pie
DEF_REQUIREMENTS="openssl libcbor zlib libfido2 libedit"
DEF_EXPORTS="c_rehash openssl scp sftp ssh ssh-add ssh-agent ssh-keygen ssh-keyscan sshd ssh-askpass ssh-sk-helper sftp-server ssh-keysign ssh-pkcs11-helper ssh-sk-helper sshd-session"
DEF_USEFUL="libexec"
