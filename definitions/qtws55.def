inherit qtbase55

DEF_FULL_NAME="Qt 5.5 Base Library + QtWebsockets"
DEF_NAME="qtws"
DEF_SHA="215f4e8d8170933d68e9395d966ecf6213fc0a74"
DEF_VERSION="5.5.0"
DEF_SOURCE_PATH="${MAIN_SOURCE_REPOSITORY}qtwebsockets-opensource-src-${DEF_VERSION}.tar.xz"
DEF_REQUIREMENTS="pkgconf make qtbase55"
DEF_CONFIGURE="no-conf"
DEF_CONFLICTS_WITH="Qt Qtbase"

if [ "${SYSTEM_NAME}" = "FreeBSD" ]; then
    DEF_AFTER_UNPACK_METHOD="${PREFIX}/bin/qmake -spec freebsd-clang qtwebsockets.pro"
else
    DEF_AFTER_UNPACK_METHOD="${PREFIX}/bin/qmake qtwebsockets.pro"
fi
