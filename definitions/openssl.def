. ${DEFINITIONS_DIR}libressl.def

APP_FULL_NAME="OpenSSL Library"
APP_SHA="2047c592a6e5a42bd37970bdb4a931428110a927"
APP_NAME="openssl"
APP_VERSION="1.0.2f"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}.tar.gz"
APP_CONFIGURE_SCRIPT="./config"
APP_CONFIGURE_ARGS="shared -L${PREFIX}/lib -I${PREFIX}/include"
if [ "${SYSTEM_NAME}" = "Darwin" ]; then
    export APP_CONFIGURE_SCRIPT="./Configure"
    export APP_CONFIGURE_ARGS="darwin64-x86_64-cc -shared"
fi
APP_REQUIREMENTS="perl"
APP_NO_CCACHE="true"
APP_EXPORTS="openssl"
APP_MAKE_METHOD="make -s"
APP_INSTALL_METHOD="make -s install"
