DISABLE_ON="Linux FreeBSD"
APP_FULL_NAME="Qt5 Library with Glib support"
APP_NAME="qt5"
APP_SHA="3cc6ebc6480e1f63b808c4476ef2053570b9a22d"
APP_VERSION="5.2.1"
APP_POSTFIX="-glib"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}qt-everywhere-opensource-src-${APP_VERSION}.tar.gz"
APP_CONFIGURE_ARGS="-prefix ${PREFIX} -glib -opensource -confirm-license -release -shared -no-largefile -no-cups -no-sql-sqlite -verbose -nomake examples -nomake tests -L${PREFIX}/lib"
APP_EXPORTS="moc lconvert lrelease lupdate qdoc3 qmake rcc"
APP_REQUIREMENTS="gmake zlib pcre pkgconf libffi gettext python-legacy libtool libiconv glib238"
APP_MAKE_METHOD="gmake -s ${MAKE_OPTS}"
APP_INSTALL_METHOD="gmake install -s ${MAKE_OPTS}"
