DEF_FULL_NAME="GNU Binutils are a collection of binary tools."
DEF_ORIGIN="http://ftp.gnu.org/gnu/binutils/"
DEF_NAME="binutils"
DEF_VERSION="2.33.1"
DEF_SHA="06598868f5fa8efc98427dcb790d42c664f1a1a4"
DEF_SOURCE_PATH="${MAIN_SOURCE_REPOSITORY}${DEF_NAME}-${DEF_VERSION}.tar.xz"
DEF_ORIGIN="https://ftp.gnu.org/gnu/${DEF_NAME}/${DEF_NAME}-${DEF_VERSION}.tar.xz"
DEF_REQUIREMENTS="pkgconf make m4 perl-static libtool bison gmp mpfr libmpc texinfo isl zlib"
DEF_CONFIGURE_ARGS="--enable-shared --with-gmp=${PREFIX} --with-mpfr=${PREFIX} --with-isl=${PREFIX} --enable-vtable-verify --disable-werror --enable-host-shared --with-build-sysroot=/ --disable-libstdcxx --disable-static"
DEF_EXPORTS="ld ld.bfd ld.gold addr2line as ar c dlltool gprof nlmconv nm objcopy objdump ranlib readelf size strings strip windmc windres"
DEF_USELESS="share/bison share/libtool share/texinfo"
