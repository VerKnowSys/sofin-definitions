DEF_FULL_NAME="Apache Maven"
DEF_NAME="maven"
DEF_SHA="bbf10d15c13aeae05fc915f096c2f816ca741093"
DEF_VERSION="3.0.5"
DEF_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${DEF_NAME}-${DEF_VERSION}-bin.tar.bz2"
DEF_CONFIGURE="binary"
DEF_AFTER_UNPACK_CALLBACK="${CP_BIN} -fR ${CACHE_DIR}cache/${DEF_NAME}-${DEF_VERSION}-${RUNTIME_SHA}/${DEF_NAME}-${DEF_VERSION}/ ${PREFIX}"
DEF_AFTER_INSTALL_CALLBACK="${FIND_BIN} ${PREFIX} -type f -name '*.dll' -o -name '*.exe' -o -name '*.bat' | ${XARGS_BIN} ${RM_BIN}"
DEF_EXPORTS="mvn mvnDebug mvnyjp"
