DISABLE_ON="Darwin"
APP_FULL_NAME="Common Unix Printing System"
APP_SHA="fa7d92954df9eb0498231c0b773f389431ede38d"
APP_NAME="cups"
APP_VERSION="1.6.4"
APP_HTTP_PATH="${MAIN_SOURCE_REPOSITORY}${APP_NAME}-${APP_VERSION}-source.tar.bz2"
APP_CONFIGURE_ARGS="--localstatedir=${PREFIX}/state --disable-gssapi --with-cups-user=nobody --with-cups-group=nobody --with-system-groups=nogroup --without-docdir --without-icondir --without-menudir --with-cachedir=${PREFIX}/cache --with-pam-module='unix' --enable-ssl"
APP_AFTER_INSTALL_CALLBACK="${MKDIR_BIN} -p ${PREFIX}/state ; ${MKDIR_BIN} -p ${PREFIX}/cache"
APP_EXPORTS="cancel cups-config cupstestdsc cupstestppd ipptool lp lpoptions lppasswd lpq lpr lprm lpstat ppdc ppdhtml ppdi ppdmerge ppdpo"
